<?xml version="1.0" encoding="UTF-8" ?>
<resources>
  <!-- Inherit from the light Material Theme -->
  <style name="MyCustomTheme" parent="android:Theme.Material.Light">
    <!-- Override the app bar color -->
    <item name="android:colorPrimary">@color/my_other_green</item>
    <!-- Override the color of UI controls -->
    <item name="android:colorAccent">@color/my_other_green</item>
  </style>

  <style name ="dialogAnimation">
    <item name="android:windowEnterAnimation">@anim/slideUp</item>
    <item name="android:windowExitAnimation">@anim/slideRight</item>
  </style>
</resources>
